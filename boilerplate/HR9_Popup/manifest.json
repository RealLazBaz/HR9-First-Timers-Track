{
	"manifest_version": 2, 
	"name": "It's Time To Stop",
	"version": "1.0",
  	"browser_action": {
  		"default_title": "Track Time",
    	"default_icon": "frank.png",
    	"default_popup": "popup.html"
  	}, 
  	"permissions": [
		  "activeTab",
		  "tabs",
		  "storage"
	  ],
	  
	  "background":
	  {
		  "scripts" : ["background.js"],
		  "persistent" : false
	  }
}